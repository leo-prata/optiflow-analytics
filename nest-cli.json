{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/optiflowanalytics/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/optiflowanalytics/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/optiflowanalytics",
  "projects": {
    "auth-api": {
      "type": "application",
      "root": "apps/auth-api",
      "entryFile": "main",
      "sourceRoot": "apps/auth-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-api/tsconfig.app.json"
      }
    },
    "dashboard-api": {
      "type": "application",
      "root": "apps/dashboard-api",
      "entryFile": "main",
      "sourceRoot": "apps/dashboard-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/dashboard-api/tsconfig.app.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "import-document": {
      "type": "application",
      "root": "apps/import-document",
      "entryFile": "main",
      "sourceRoot": "apps/import-document/src",
      "compilerOptions": {
        "tsConfigPath": "apps/import-document/tsconfig.app.json"
      }
    },
    "optiflowanalytics": {
      "type": "application",
      "root": "apps/optiflowanalytics",
      "entryFile": "main",
      "sourceRoot": "apps/optiflowanalytics/src",
      "compilerOptions": {
        "tsConfigPath": "apps/optiflowanalytics/tsconfig.app.json"
      }
    },
    "solver-process": {
      "type": "application",
      "root": "apps/solver-process",
      "entryFile": "main",
      "sourceRoot": "apps/solver-process/src",
      "compilerOptions": {
        "tsConfigPath": "apps/solver-process/tsconfig.app.json"
      }
    },
    "user-api": {
      "type": "application",
      "root": "apps/user-api",
      "entryFile": "main",
      "sourceRoot": "apps/user-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-api/tsconfig.app.json"
      }
    }
  }
}